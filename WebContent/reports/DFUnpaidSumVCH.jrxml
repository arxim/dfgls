	</parameter>
	<parameter name="department_code" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<parameter name="payor_code" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT S.DOCTOR_CODE, D.NAME_THAI, 
SUM(S.AMOUNT_AFT_DISCOUNT) AS AMOUNT_AFT_DISCOUNT, SUM(S.DR_AMT) AS DR_AMT,
SUM(S.DR_TAX_400+S.DR_TAX_401+S.DR_TAX_402+S.DR_TAX_406) AS DR_TAX_AMT
FROM TRN_DAILY S LEFT OUTER JOIN DOCTOR D ON (S.DOCTOR_CODE = D.CODE
AND S.HOSPITAL_CODE = D.HOSPITAL_CODE)
WHERE (S.INVOICE_DATE BETWEEN $P{from_date} AND $P{to_date}) AND
D.DOCTOR_PROFILE_CODE LIKE $P{doctor} AND INVOICE_TYPE <> 'ORDER' AND 
(S.PAY_BY_CASH = 'N' AND S.PAY_BY_AR = 'N' AND S.PAY_BY_DOCTOR = 'N' AND
S.PAY_BY_PAYOR = 'N' AND S.PAY_BY_CASH_AR = 'N') AND
S.IS_PAID <> 'N' AND D.ACTIVE = '1' AND S.ACTIVE = '1' AND
S.ORDER_ITEM_ACTIVE = '1' AND S.PAYOR_OFFICE_CODE LIKE $P{payor_code} AND
S.PATIENT_DEPARTMENT_CODE LIKE $P{department_code} AND
(S.BATCH_NO = '' OR S.BATCH_NO IS NULL) AND
S.HOSPITAL_CODE LIKE $P{hospital_code}
GROUP BY S.DOCTOR_CODE, D.NAME_THAI
ORDER BY S.DOCTOR_CODE]]></queryString>

