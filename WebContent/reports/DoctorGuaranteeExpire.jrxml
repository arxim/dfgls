	<queryString><![CDATA[SELECT DR.DOCTOR_PROFILE_CODE, DP.NAME_THAI AS PROFILE_NAME, DR.CODE,
CASE WHEN DR.ACTIVE = '1' THEN 'ACTIVE' ELSE 'IN ACTIVE' END AS ACTIVE, 
DR.NAME_THAI, DR.GUARANTEE_START_DATE, DR.GUARANTEE_EXPIRE_DATE, DR.DOCTOR_CATEGORY_CODE
FROM DOCTOR DR LEFT OUTER JOIN DOCTOR_PROFILE DP ON DR.DOCTOR_PROFILE_CODE = DP.CODE
AND DR.HOSPITAL_CODE = DP.HOSPITAL_CODE
WHERE LEN(DR.GUARANTEE_START_DATE) = 8
AND DR.GUARANTEE_EXPIRE_DATE <= $P{to_date}
AND DR.HOSPITAL_CODE LIKE $P{hospital_code}
AND DR.ACTIVE = '1'
ORDER BY DR.GUARANTEE_EXPIRE_DATE DESC, DR.DOCTOR_PROFILE_CODE, DR.CODE]]></queryString>

