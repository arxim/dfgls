		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT SG.GUARANTEE_DR_CODE, SG.GUARANTEE_TYPE_CODE, SG.GUARANTEE_LOCATION, SG.GL_DEPARTMENT_CODE,
DPT.DESCRIPTION AS DEPARTMENT_NAME,
CASE WHEN SG.ADMISSION_TYPE_CODE = 'U' THEN 'ALL' ELSE SG.ADMISSION_TYPE_CODE END AS ADMISSION_TYPE_CODE,
SG.MM, SG.YYYY, SG.START_DATE, SG.START_TIME,
CASE WHEN SG.EARLY_TIME = '000000' OR SG.EARLY_TIME = START_TIME THEN '' ELSE SG.EARLY_TIME END AS EARLY_TIME, 
SG.END_DATE, SG.END_TIME, 
CASE WHEN SG.LATE_TIME = '000000' OR SG.LATE_TIME = SG.END_TIME THEN '' ELSE SG.LATE_TIME END AS LATE_TIME,
SG.GUARANTEE_AMOUNT, SG.GUARANTEE_FIX_AMOUNT, SG.GUARANTEE_INCLUDE_AMOUNT, SG.GUARANTEE_EXCLUDE_AMOUNT,
DR.NAME_THAI, SG.USER_ID, CASE WHEN SG.ACTIVE = '0' THEN 'INACTIVE' ELSE 'ACTIVE' END AS ACTIVE  , HP.DESCRIPTION_THAI
FROM STP_GUARANTEE SG 
LEFT OUTER JOIN DOCTOR DR ON SG.GUARANTEE_DR_CODE = DR.CODE AND SG.HOSPITAL_CODE = DR.HOSPITAL_CODE
LEFT OUTER JOIN DEPARTMENT DPT ON SG.GL_DEPARTMENT_CODE = DPT.CODE AND SG.HOSPITAL_CODE = DPT.HOSPITAL_CODE
INNER JOIN HOSPITAL HP ON SG.HOSPITAL_CODE = HP.CODE
WHERE 
(SG.GUARANTEE_DR_CODE BETWEEN $P{from_doctor} AND $P{to_doctor}) AND 
SG.YYYY LIKE $P{year} AND SG.MM LIKE $P{month}
AND SG.HOSPITAL_CODE LIKE $P{hospital_code}
AND SG.GL_DEPARTMENT_CODE LIKE $P{guarantee_department_code}
ORDER BY SG.GL_DEPARTMENT_CODE, SG.GUARANTEE_DR_CODE, SG.ACTIVE DESC, SG.START_DATE]]></queryString>

