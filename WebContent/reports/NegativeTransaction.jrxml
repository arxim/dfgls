	<queryString><![CDATA[SELECT I.HOSPITAL_CODE, CASE WHEN I.ADMISSION_TYPE_CODE = 'I' THEN 'IPD' ELSE 'OPD' END AS ADMISSION_TYPE_CODE,
I.BILL_NO, I.BILL_DATE, I.LINE_NO, I.RECEIPT_TYPE_CODE, I.HN_NO, I.PATIENT_NAME, I.EPISODE_NO, I.PAYOR_CODE,
I.PAYOR_NAME, I.ORDER_ITEM_CODE, I.ORDER_ITEM_DESCRIPTION, I.DOCTOR_CODE, I.DOCTOR_NAME, 
I.AMOUNT_BEF_DISCOUNT, I.AMOUNT_OF_DISCOUNT, D.DEPARTMENT_CODE
FROM INT_HIS_BILL I LEFT OUTER JOIN DOCTOR D ON I.DOCTOR_CODE = D.CODE
WHERE (I.BILL_DATE BETWEEN $P{from_date} AND $P{to_date}) AND
I.DOCTOR_CODE LIKE $P{doctor_code} AND
I.ADMISSION_TYPE_CODE LIKE $P{admission_type} AND
I.TRANSACTION_TYPE LIKE $P{transaction_type} AND
I.ORDER_ITEM_CODE LIKE $P{order_item} AND
I.AMOUNT_BEF_DISCOUNT-I.AMOUNT_OF_DISCOUNT < 0 AND
I.HOSPITAL_CODE LIKE $P{hospital_code}
ORDER BY I.ADMISSION_TYPE_CODE, I.BILL_NO, I.LINE_NO]]></queryString>

