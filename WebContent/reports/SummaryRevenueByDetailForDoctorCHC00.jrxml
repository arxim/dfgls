	<parameter name="year" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["2017"]]></defaultValueExpression>
	</parameter>
	<parameter name="month" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["03"]]></defaultValueExpression>
	</parameter>
	<parameter name="from_doctor" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["04DN0155"]]></defaultValueExpression>
	</parameter>
	<parameter name="to_doctor" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["04DN0155"]]></defaultValueExpression>
	</parameter>
	<parameter name="hospital_code" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["004"]]></defaultValueExpression>
	</parameter>
	<parameter name="doctor_category" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<parameter name="doctor_department" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<parameter name="order_item" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<parameter name="order_item_category" isForPrompting="false" class="java.lang.String">
