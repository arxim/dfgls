	</parameter>
	<parameter name="term" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[(SELECT 
		DP.CODE AS PROFILE_CODE,
		DP.NAME_THAI AS PROFILE_NAME,
		2 AS 'TYPE_QUERY',
		--CASE WHEN SD.GUARANTEE_TYPE != '' THEN SUBSTRING(SD.GUARANTEE_DATE_TIME,0,9) ELSE
		CASE WHEN SD.GUARANTEE_TYPE != '' AND SD.GUARANTEE_TYPE <> 'MLY' AND SD.HOSPITAL_CODE <> '004' THEN SUBSTRING(SD.GUARANTEE_DATE_TIME,0,9) ELSE
		SD.VERIFY_DATE END AS 'VERIFY_DATE',
		SD.DOCTOR_CODE AS 'DOCTOR_CODE',
		SD.VERIFY_TIME AS 'START_TIME',
		'' AS 'END_DATE',
		'' AS 'END_TIME',
		--SD.GUARANTEE_DATE_TIME AS 'DATE_TIME',
		CASE WHEN SD.GUARANTEE_TYPE = 'MLY' AND SD.HOSPITAL_CODE = '004' THEN SD.VERIFY_DATE ELSE SD.GUARANTEE_DATE_TIME END AS 'DATE_TIME',
		0.00 AS 'ABSORB_AMOUNT',
		SD.YYYY AS 'YYYY', 
		SD.MM AS 'MM', 
		DR.NAME_THAI AS 'NAME_THAI',
		DR.DOCTOR_CATEGORY_CODE,
		SD.INVOICE_NO AS 'INVOICE_NO', 
		SD.INVOICE_DATE AS 'INVOICE_DATE',
		SUBSTRING(SD.PATIENT_NAME,0,40) AS 'PATIENT_NAME',
		SD.HN_NO AS 'HN_NO', 
		SD.IS_PAID, 
		SD.AMOUNT_AFT_DISCOUNT AS 'OLD_AMOUNT',
		SD.AMOUNT_BEF_DISCOUNT-SD.AMOUNT_OF_DISCOUNT AS 'WRITE_OFF_AMOUNT', 
		SD.INVOICE_TYPE,
		SD.LINE_NO AS 'LINE_NO',
		SD.ORDER_ITEM_CODE AS 'ORDER_ITEM_CODE', 
		SUBSTRING(OI.DESCRIPTION_THAI,0,30) AS 'ORDER_ITEM_NAME',
		CASE WHEN SD.GUARANTEE_NOTE = 'ABSORB GUARANTEE' THEN SD.DR_AMT ELSE SD.DR_AMT END AS 'DR_AMT',
		SD.GUARANTEE_NOTE,
		SD.IS_WRITE_OFF AS 'IS_WRITE_OFF', 
		SD.IS_PARTIAL, SD.GUARANTEE_PAID_AMT,
		CASE WHEN (SD.PAY_BY_AR = 'Y' AND SD.IS_PARTIAL = 'N' AND GUARANTEE_NOTE = '') THEN 'A' ELSE CASE WHEN (SD.PAY_BY_AR = 'Y' AND SD.IS_PARTIAL = 'Y' AND SD.GUARANTEE_NOTE = '') THEN 'B' ELSE 'C' END END AS PAY_METHOD,
		SD.DR_TAX_406 AS DR_TAX_AMT, SD.DR_TAX_402, SD.OLD_DR_AMT ,
		CASE WHEN SD.GUARANTEE_NOTE LIKE '%EXTRA%' THEN 'EXT' ELSE SD.GUARANTEE_TYPE END AS GUARANTEE_TYPE_CODE , 
		SD.GUARANTEE_CODE AS GUARANTEE_CODE , 
		CASE WHEN SD.GUARANTEE_CODE = '' THEN  '1' ELSE  '0' END AS SHORT_TRANSACTION , 
		ISNULL (STP.GUARANTEE_AMOUNT ,0) AS GUARANTEE_AMOUNT ,
		ISNULL (STP.GUARANTEE_INCLUDE_AMOUNT, 0) AS GUARANTEE_INCLUDE_AMOUNT,
		ISNULL (STP.START_DATE , '' ) AS GUARANTEE_START_DATE , 
		ISNULL (STP.START_TIME , '' ) AS GUARANTEE_START_TIME , 
		ISNULL (STP.END_DATE , '' ) AS GUARANTEE_END_DATE , 
		ISNULL (STP.END_TIME , '' ) AS GUARANTEE_END_TIME , 
		HP.DESCRIPTION_THAI 
	FROM TRN_PAYMENT SD LEFT OUTER JOIN DOCTOR DR ON (SD.DOCTOR_CODE = DR.CODE AND SD.HOSPITAL_CODE = DR.HOSPITAL_CODE)
	LEFT OUTER JOIN DOCTOR_PROFILE DP ON (DR.DOCTOR_PROFILE_CODE = DP.CODE AND DR.HOSPITAL_CODE = DP.HOSPITAL_CODE)
	LEFT OUTER JOIN ORDER_ITEM OI ON (OI.CODE = SD.ORDER_ITEM_CODE	AND OI.HOSPITAL_CODE = SD.HOSPITAL_CODE)
	LEFT OUTER JOIN HOSPITAL HP ON (SD.HOSPITAL_CODE = HP.CODE)
	LEFT OUTER JOIN STP_GUARANTEE STP ON SD.GUARANTEE_CODE = STP.GUARANTEE_CODE  AND  SD.GUARANTEE_DR_CODE  =   STP.GUARANTEE_DR_CODE 
	AND SD.GUARANTEE_TERM_MM = STP.MM AND SD.GUARANTEE_TERM_YYYY = STP.YYYY AND SD.HOSPITAL_CODE = STP.HOSPITAL_CODE
	AND SD.GUARANTEE_DATE_TIME = STP.START_DATE+START_TIME AND STP.ACTIVE = '1'
	WHERE SD.HOSPITAL_CODE = $P{hospital_code}
	AND (DP.CODE BETWEEN $P{from_doctor} AND $P{to_doctor})
	AND SD.DOCTOR_CODE IN (SELECT DOCTOR_CODE FROM SUMMARY_PAYMENT WHERE YYYY = $P{year} AND MM = $P{month})
	AND ((SD.YYYY LIKE $P{year} AND SD.MM LIKE $P{month})
	OR (SD.GUARANTEE_TERM_YYYY LIKE $P{year} AND SD.GUARANTEE_TERM_MM LIKE $P{month} AND (SD.GUARANTEE_NOTE LIKE '%SOME%' OR SD.GUARANTEE_NOTE = 'ABSORB GUARANTEE')))
	AND DR.DOCTOR_CATEGORY_CODE LIKE $P{doctor_category}
	AND DR.DEPARTMENT_CODE LIKE $P{doctor_department}
	AND SD.ACTIVE = '1' AND SD.ORDER_ITEM_ACTIVE = '1' --AND STP.ACTIVE = '1'
	AND OI.IS_COMPUTE = 'Y'
	AND SD.ORDER_ITEM_CODE LIKE $P{order_item}
	AND OI.ORDER_ITEM_CATEGORY_CODE LIKE $P{order_item_category}
	AND SD.COMPUTE_DAILY_DATE is not null AND SD.COMPUTE_DAILY_DATE != ''
	AND SD.DR_AMT > 0
	AND (SD.GUARANTEE_NOTE LIKE '%OLD%' OR SD.IS_PAID = 'Y')
	AND SD.PAYMENT_DATE LIKE $P{payment_date}
	AND SD.PAYMENT_TERM LIKE $P{term}
	)
UNION ALL
	(SELECT top 3000
		DP.CODE AS PROFILE_CODE,
		DP.NAME_THAI AS PROFILE_NAME,
		1 AS 'TYPE_QUERY',		
		STP.START_DATE AS 'VERIFY_DATE',
		STP.GUARANTEE_DR_CODE AS 'DOCTOR_CODE',
		STP.START_TIME AS 'START_TIME',
		STP.END_DATE AS 'END_DATE',
		STP.END_TIME AS 'END_TIME',
		STP.START_DATE+STP.START_TIME AS 'DATE_TIME',
		CASE WHEN HP402_ABSORB_AMOUNT > 0 
			THEN (HP402_ABSORB_AMOUNT)
			ELSE DF402_CASH_AMOUNT
		END AS 'ABSORB_AMOUNT',
		'' AS 'YYYY', 
		'' AS 'MM', 
		DR.NAME_THAI AS 'NAME_THAI', 
		'' AS 'DOCTOR_CATEGORY_CODE',
		'' AS 'INVOICE_NO',
		'' AS 'INVOICE_DATE',
		'' AS 'PATIENT_NAME',
		'' AS 'HN_NO', 
		'' AS 'IS_PAID', 
		0.00 AS 'OLD_AMOUNT', 
		0.00 AS 'WRITE_OFF_AMOUNT',
		CASE WHEN STP.GUARANTEE_EXCLUDE_AMOUNT > 0 
			THEN STP.GUARANTEE_TYPE_CODE+' : Special Consult'
			ELSE CASE WHEN STP.HOSPITAL_CODE = '00002' 
				  THEN STP.GUARANTEE_TYPE_CODE+' : Special Doctorfee'
				  ELSE STP.GUARANTEE_TYPE_CODE+' : Absorb Guarantee' END
		END AS 'INVOICE_TYPE',
		'' AS 'LINE_NO',
		'' AS 'ORDER_ITEM_CODE', 
		'' AS 'ORDER_ITEM_NAME',
		0.00 AS 'DR_AMT', 
		'' AS 'GUARANTEE_NOTE',
		'N' AS 'IS_WRITE_OFF',
		'N' AS 'IS_PARTIAL', 
		0.00 AS 'GUARANTEE_PAID_AMT',
		'C' AS 'PAY_METHOD',
		ISNULL((
			 SELECT SUM(CASE WHEN TAX_TYPE_CODE = '406' THEN
			 ISNULL(TAX_AMOUNT , 0) 
			 ELSE 0 END)
			 FROM TRN_EXPENSE_DETAIL TED
			 WHERE TED.HOSPITAL_CODE = STP.HOSPITAL_CODE 
			 AND TED.LINE_NO = STP.GUARANTEE_CODE 
			 AND TED.DOCTOR_CODE = STP.GUARANTEE_DR_CODE
			 AND TED.YYYY + TED.MM = STP.YYYY + STP.MM
		) , 0) AS 'DR_TAX_AMT',
		ISNULL((
			 SELECT SUM(CASE WHEN TAX_TYPE_CODE = '402' THEN
			 ISNULL(TAX_AMOUNT , 0) 
			 ELSE 0 END)
			 FROM TRN_EXPENSE_DETAIL TED
			 WHERE TED.HOSPITAL_CODE = STP.HOSPITAL_CODE 
			 AND TED.LINE_NO = STP.GUARANTEE_CODE 
			 AND TED.DOCTOR_CODE = STP.GUARANTEE_DR_CODE
			 AND TED.YYYY + TED.MM = STP.YYYY + STP.MM
		) , 0) AS 'DR_TAX_402',
		0.00 AS 'OLD_DR_AMT' , 
		CASE WHEN STP.GUARANTEE_AMOUNT = 0 THEN 'EXT' ELSE STP.GUARANTEE_TYPE_CODE END AS GUARANTEE_TYPE_CODE , 
		STP.GUARANTEE_CODE  AS 'GUARANTEE_CODE' , 
		'0' AS SHORT_TRANSACTION , 
		STP.GUARANTEE_AMOUNT+STP.GUARANTEE_EXCLUDE_AMOUNT AS GUARANTEE_AMOUNT , 
		STP.GUARANTEE_INCLUDE_AMOUNT,
		STP.START_DATE AS GUARANTEE_START_DATE , 
		STP.START_TIME AS GUARANTEE_START_TIME , 
		STP.END_DATE AS GUARANTEE_END_DATE , 
		STP.END_TIME AS GUARANTEE_END_TIME , 
		HP.DESCRIPTION_THAI 
	FROM STP_GUARANTEE STP 
	LEFT OUTER JOIN DOCTOR DR ON (STP.GUARANTEE_DR_CODE = DR.CODE AND STP.HOSPITAL_CODE = DR.HOSPITAL_CODE)
	LEFT OUTER JOIN DOCTOR_PROFILE DP ON (DR.DOCTOR_PROFILE_CODE = DP.CODE AND
	DR.HOSPITAL_CODE = DP.HOSPITAL_CODE)
	INNER JOIN HOSPITAL HP ON STP.HOSPITAL_CODE = HP.CODE
	WHERE 
	--(STP.DF402_CASH_AMOUNT+STP.HP402_ABSORB_AMOUNT+STP.DF406_HOLD_AMOUNT >0) AND 
	STP.GUARANTEE_TYPE_CODE = 'DLY'
	AND (DR.DOCTOR_PROFILE_CODE BETWEEN $P{from_doctor} AND $P{to_doctor})
	AND STP.YYYY LIKE $P{year}
	AND STP.MM LIKE $P{month}
	AND STP.ACTIVE = '1'
	AND STP.HOSPITAL_CODE LIKE $P{hospital_code}
	AND DR.DOCTOR_CATEGORY_CODE LIKE $P{doctor_category}
	AND DR.DEPARTMENT_CODE LIKE $P{doctor_department})
UNION ALL
	(SELECT DISTINCT
		DP.CODE AS PROFILE_CODE,
		DP.NAME_THAI AS PROFILE_NAME,
		1 AS 'TYPE_QUERY',		
		STP.START_DATE AS 'VERIFY_DATE',
		STP.GUARANTEE_DR_CODE AS 'DOCTOR_CODE',
		'000000' AS 'START_TIME',
		STP.YYYY+STP.MM+'01' AS 'END_DATE',
		'000000' AS 'END_TIME',
		--STP.START_DATE+STP.START_TIME AS 'DATE_TIME',
		CASE WHEN STP.GUARANTEE_TYPE_CODE = 'MLY' AND STP.HOSPITAL_CODE = '004' THEN STP.START_DATE ELSE STP.START_DATE+STP.START_TIME END AS 'DATE_TIME',
		CASE WHEN HP402_ABSORB_AMOUNT > 0 
			THEN (HP402_ABSORB_AMOUNT)
			ELSE DF402_CASH_AMOUNT
		END AS 'ABSORB_AMOUNT',
		'' AS 'YYYY', 
		'' AS 'MM', 
		DR.NAME_THAI AS 'NAME_THAI', 
		'' AS 'DOCTOR_CATEGORY_CODE',
		'' AS 'INVOICE_NO',
		'' AS 'INVOICE_DATE',
		'' AS 'PATIENT_NAME',
		'' AS 'HN_NO', 
		'' AS 'IS_PAID', 
		0.00 AS 'OLD_AMOUNT', 
		0.00 AS 'WRITE_OFF_AMOUNT',
		CASE WHEN STP.GUARANTEE_EXCLUDE_AMOUNT > 0 
			THEN STP.GUARANTEE_TYPE_CODE+' : Special Consult'
			ELSE CASE WHEN STP.HOSPITAL_CODE = '00002' 
				  THEN STP.GUARANTEE_TYPE_CODE+' : Special Doctorfee'
				  ELSE STP.GUARANTEE_TYPE_CODE+' : Absorb Guarantee' END
		END AS 'INVOICE_TYPE',
		'' AS 'LINE_NO',
		'' AS 'ORDER_ITEM_CODE', 
		'' AS 'ORDER_ITEM_NAME',
		0.00 AS 'DR_AMT', 
		'' AS 'GUARANTEE_NOTE',
		'N' AS 'IS_WRITE_OFF', 
		'N' AS 'IS_PARTIAL',
		0.00 AS 'GUARANTEE_PAID_AMT',
		'C' AS 'PAY_METHOD',
		ISNULL((
			 SELECT SUM(CASE WHEN TAX_TYPE_CODE = '406' THEN
			 ISNULL(TAX_AMOUNT , 0)
			 ELSE 0 END)
			 FROM TRN_EXPENSE_DETAIL TED
			 WHERE TED.HOSPITAL_CODE = STP.HOSPITAL_CODE 
			 AND TED.LINE_NO = STP.GUARANTEE_CODE + STP.IS_INCLUDE_LOCATION + STP.GUARANTEE_LOCATION
			 AND TED.DOCTOR_CODE = STP.GUARANTEE_DR_CODE
			 AND TED.YYYY + TED.MM = STP.YYYY + STP.MM
		) , 0) AS 'DR_TAX_AMT',
		ISNULL((
			 SELECT SUM(CASE WHEN TAX_TYPE_CODE = '402' THEN
			 ISNULL(TAX_AMOUNT , 0)
			 ELSE 0 END)
			 FROM TRN_EXPENSE_DETAIL TED
			 WHERE TED.HOSPITAL_CODE = STP.HOSPITAL_CODE 
			 AND TED.LINE_NO = STP.GUARANTEE_CODE + STP.IS_INCLUDE_LOCATION + STP.GUARANTEE_LOCATION
			 AND TED.DOCTOR_CODE = STP.GUARANTEE_DR_CODE
			 AND TED.YYYY + TED.MM = STP.YYYY + STP.MM
		) , 0) AS 'DR_TAX_402',
		0.00 AS 'OLD_DR_AMT' , 
		STP.GUARANTEE_TYPE_CODE AS GUARANTEE_TYPE_CODE , 
		STP.GUARANTEE_CODE AS 'GUARANTEE_CODE' , 
		'0' AS SHORT_TRANSACTION , 
		STP.GUARANTEE_AMOUNT+STP.GUARANTEE_EXCLUDE_AMOUNT AS GUARANTEE_AMOUNT , 
		STP.GUARANTEE_INCLUDE_AMOUNT,
		STP.START_DATE AS GUARANTEE_START_DATE , 
 		STP.START_TIME AS GUARANTEE_END_TIME , 
		STP.END_DATE AS GUARANTEE_END_DATE , 
		STP.END_TIME AS GUARANTEE_END_TIME , 
		HP.DESCRIPTION_THAI
	FROM STP_GUARANTEE STP 
	LEFT OUTER JOIN DOCTOR DR ON (STP.GUARANTEE_DR_CODE = DR.CODE AND STP.HOSPITAL_CODE = DR.HOSPITAL_CODE)
	LEFT OUTER JOIN DOCTOR_PROFILE DP ON (DR.DOCTOR_PROFILE_CODE = DP.CODE AND
	DR.HOSPITAL_CODE = DP.HOSPITAL_CODE)
	INNER JOIN HOSPITAL HP ON STP.HOSPITAL_CODE = HP.CODE 
	WHERE 
	--(STP.DF402_CASH_AMOUNT+STP.HP402_ABSORB_AMOUNT+STP.DF406_HOLD_AMOUNT >0) AND 
	STP.GUARANTEE_TYPE_CODE <> 'DLY'
	AND (DR.DOCTOR_PROFILE_CODE BETWEEN $P{from_doctor} AND $P{to_doctor})
	AND STP.YYYY LIKE $P{year}
	AND STP.MM LIKE $P{month}
	AND STP.ACTIVE = '1'
	AND STP.HOSPITAL_CODE LIKE $P{hospital_code}
	AND DR.DOCTOR_CATEGORY_CODE LIKE $P{doctor_category}
	AND DR.DEPARTMENT_CODE LIKE $P{doctor_department})
ORDER BY PROFILE_CODE, DOCTOR_CODE DESC, PAY_METHOD, GUARANTEE_TYPE_CODE, DATE_TIME, VERIFY_DATE, INVOICE_DATE, HN_NO, INVOICE_NO, LINE_NO, SHORT_TRANSACTION]]></queryString>

