	<queryString><![CDATA[(SELECT 
		DP.CODE AS PROFILE_CODE,
		PO.NAME_THAI AS PROFILE_NAME,
		2 AS 'TYPE_QUERY',
		CASE WHEN SD.GUARANTEE_TYPE != '' THEN SUBSTRING(SD.GUARANTEE_DATE_TIME,0,9) ELSE
		SD.VERIFY_DATE END AS 'VERIFY_DATE',
		SD.DOCTOR_CODE AS 'DOCTOR_CODE',
		SD.VERIFY_TIME AS 'START_TIME',
		'' AS 'END_DATE',
		'' AS 'END_TIME',
		SD.GUARANTEE_DATE_TIME AS 'DATE_TIME',
		SD.ADMISSION_TYPE_CODE,
		SD.EPISODE_NO,
		0.00 AS 'ABSORB_AMOUNT',
		SD.YYYY AS 'YYYY', 
		SD.MM AS 'MM', 
		D.NAME_THAI AS 'NAME_THAI',
		D.DOCTOR_CATEGORY_CODE,
		SD.INVOICE_NO AS 'INVOICE_NO', 
		SD.INVOICE_DATE AS 'INVOICE_DATE',
		SUBSTRING(SD.PATIENT_NAME,0,40) AS 'PATIENT_NAME',
		SD.HN_NO AS 'HN_NO', 
		SD.IS_PAID, 
		SD.AMOUNT_AFT_DISCOUNT AS 'OLD_AMOUNT',
		SD.AMOUNT_BEF_DISCOUNT-SD.AMOUNT_OF_DISCOUNT AS 'WRITE_OFF_AMOUNT', 
		SD.INVOICE_TYPE,
		SD.LINE_NO AS 'LINE_NO',
		SD.ORDER_ITEM_CODE AS 'ORDER_ITEM_CODE', 
		SUBSTRING(OI.DESCRIPTION_THAI,0,30) AS 'ORDER_ITEM_NAME',
		SD.DR_AMT AS 'DR_AMT',
		SD.GUARANTEE_NOTE,
		SD.IS_WRITE_OFF AS 'IS_WRITE_OFF', 
		SD.IS_PARTIAL, SD.GUARANTEE_PAID_AMT,
		CASE WHEN SD.PAY_BY_AR = 'Y' THEN 'A' ELSE 'C' END AS PAY_METHOD,
		SD.DR_TAX_406 AS DR_TAX_AMT, SD.DR_TAX_402, SD.OLD_DR_AMT ,
		CASE WHEN SD.GUARANTEE_NOTE LIKE '%EXTRA%' THEN 'EXT' ELSE CASE WHEN SD.GUARANTEE_TERM_YYYY+SD.GUARANTEE_TERM_MM <> $P{year}+$P{month} THEN '' ELSE SD.GUARANTEE_TYPE END END AS GUARANTEE_TYPE_CODE , 
		SD.GUARANTEE_CODE AS GUARANTEE_CODE , 
	    	CASE WHEN SD.GUARANTEE_TERM_YYYY+SD.GUARANTEE_TERM_MM <> $P{year}+$P{month} THEN '1' 
		ELSE CASE WHEN SD.GUARANTEE_CODE = '' THEN  '1' ELSE  '0' END END AS SHORT_TRANSACTION , 
		
		HP.DESCRIPTION_THAI
		
	FROM TRN_DAILY SD LEFT OUTER JOIN DOCTOR D ON SD.DOCTOR_CODE = D.CODE AND SD.HOSPITAL_CODE = D.HOSPITAL_CODE
	LEFT OUTER JOIN ORDER_ITEM OI ON SD.ORDER_ITEM_CODE = OI.CODE AND SD.HOSPITAL_CODE = OI.HOSPITAL_CODE
	LEFT OUTER JOIN PAYOR_OFFICE PO ON SD.PAYOR_OFFICE_CODE = PO.CODE AND SD.HOSPITAL_CODE = PO.HOSPITAL_CODE
	LEFT OUTER JOIN DOCTOR_PROFILE DP ON (D.DOCTOR_PROFILE_CODE = DP.CODE AND D.HOSPITAL_CODE =DP.HOSPITAL_CODE)
	LEFT OUTER JOIN HOSPITAL HP ON (SD.HOSPITAL_CODE = HP.CODE)
		

	
	WHERE D.DOCTOR_PROFILE_CODE BETWEEN $P{from_doctor} AND $P{to_doctor} AND 
	D.DEPARTMENT_CODE LIKE $P{doctor_department} AND
	SD.ORDER_ITEM_CODE LIKE $P{order_item} AND
	D.DOCTOR_CATEGORY_CODE LIKE $P{doctor_category} AND
	OI.ORDER_ITEM_CATEGORY_CODE LIKE $P{order_item_category} AND
	SD.HOSPITAL_CODE LIKE $P{hospital_code}
	)
ORDER BY SD.ADMISSION_TYPE_CODE,SD.INVOICE_NO]]></queryString>

