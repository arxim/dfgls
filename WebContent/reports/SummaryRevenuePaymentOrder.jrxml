	<queryString><![CDATA[SELECT SM.YYYY, SM.MM, SM.DOCTOR_CODE, DR.NAME_THAI, DR.DOCTOR_TYPE_CODE,
SM.SUM_AMT AS 'SUM_AMT', 
SM.SUM_DR_AMT  AS DR_SUM_AMT, 
SM.EXDR_400, SM.EXDR_401, SM.EXDR_402, SM.EXDR_406,
ISNULL(PM.CODE , '' ) AS PAYMENT_MODE_CODE,
ISNULL(PM.DESCRIPTION , '' ) AS DESCRIPTION , SM.DR_NET_PAID_AMT,
SM.REF_PAID_NO, DP.CODE, DP.NAME_THAI AS PROFILE_NAME, 
ISNULL(SG.GUARANTEE_AMOUNT,0) AS GUARANTEE_AMOUNT, 
ISNULL(SG.GUARANTEE_EXCLUDE_AMOUNT,0) AS EXCLUDE_AMOUNT,
CASE WHEN DR.HOSPITAL_UNIT_CODE = '' THEN 'UNKNOW' ELSE DR.HOSPITAL_UNIT_CODE END AS HOSPITAL_UNIT_CODE, 
HU.DESCRIPTION AS H_UNIT,
ISNULL((SELECT SUM(ED.AMOUNT) FROM TRN_EXPENSE_DETAIL ED LEFT OUTER JOIN EXPENSE EX 
ON ED.EXPENSE_CODE = EX.CODE AND ED.HOSPITAL_CODE = EX.HOSPITAL_CODE 
WHERE ED.HOSPITAL_CODE = $P{hospital_code} AND ED.YYYY = $P{year} AND ED.MM = $P{month} AND ED.PAYMENT_TERM LIKE $P{term}
AND ED.DOCTOR_CODE = SM.DOCTOR_CODE AND EX.ADJUST_TYPE NOT IN ('HP','DR','EX') AND ED.EXPENSE_SIGN = '-1'
GROUP BY ED.DOCTOR_CODE),0) AS EXCR_AMT,
ISNULL((SELECT SUM(ED.AMOUNT) FROM TRN_EXPENSE_DETAIL ED LEFT OUTER JOIN EXPENSE EX 
ON ED.EXPENSE_CODE = EX.CODE AND ED.HOSPITAL_CODE = EX.HOSPITAL_CODE 
WHERE ED.HOSPITAL_CODE = $P{hospital_code} AND ED.YYYY = $P{year} AND ED.MM = $P{month} AND ED.PAYMENT_TERM LIKE $P{term}
AND ED.DOCTOR_CODE = SM.DOCTOR_CODE AND EX.ADJUST_TYPE NOT IN ('HP','DR','EX') AND ED.EXPENSE_SIGN = '1'
GROUP BY ED.DOCTOR_CODE),0) AS EXDR_AMT,
ISNULL((SELECT SUM(ED.AMOUNT) FROM TRN_EXPENSE_DETAIL ED LEFT OUTER JOIN EXPENSE EX
ON ED.EXPENSE_CODE = EX.CODE AND ED.HOSPITAL_CODE = EX.HOSPITAL_CODE 
WHERE ED.HOSPITAL_CODE = $P{hospital_code} AND ED.YYYY = $P{year} AND ED.MM = $P{month} AND ED.PAYMENT_TERM LIKE $P{term}
AND ED.DOCTOR_CODE = SM.DOCTOR_CODE AND EX.ADJUST_TYPE = 'HP'
GROUP BY ED.DOCTOR_CODE),0) AS GDR_402,
ISNULL((SELECT SUM(ED.AMOUNT) FROM TRN_EXPENSE_DETAIL ED LEFT OUTER JOIN EXPENSE EX 
ON ED.EXPENSE_CODE = EX.CODE AND ED.HOSPITAL_CODE = EX.HOSPITAL_CODE 
WHERE ED.HOSPITAL_CODE = $P{hospital_code} AND ED.YYYY = $P{year} AND ED.MM = $P{month}
AND ED.DOCTOR_CODE = SM.DOCTOR_CODE AND EX.ADJUST_TYPE = 'DR'
GROUP BY ED.DOCTOR_CODE),0) AS GDR_406,
ISNULL((SELECT SUM(ED.AMOUNT) FROM TRN_EXPENSE_DETAIL ED LEFT OUTER JOIN EXPENSE EX
ON ED.EXPENSE_CODE = EX.CODE AND ED.HOSPITAL_CODE = EX.HOSPITAL_CODE 
WHERE ED.HOSPITAL_CODE = $P{hospital_code} AND ED.YYYY = $P{year} AND ED.MM = $P{month} AND ED.PAYMENT_TERM LIKE $P{term}
AND ED.DOCTOR_CODE = SM.DOCTOR_CODE AND EX.ADJUST_TYPE = 'EX'
GROUP BY ED.DOCTOR_CODE),0) AS GUARANTEE_EXCLUDE_AMOUNT , HP.DESCRIPTION_THAI
FROM SUMMARY_PAYMENT SM
LEFT OUTER JOIN DOCTOR DR ON (SM.DOCTOR_CODE = DR.CODE AND SM.HOSPITAL_CODE = DR.HOSPITAL_CODE)
LEFT OUTER JOIN PAYMENT_MODE PM ON (SM.PAYMENT_MODE_CODE = PM.CODE)
LEFT OUTER JOIN DOCTOR_PROFILE DP ON (DR.DOCTOR_PROFILE_CODE = DP.CODE AND
DR.HOSPITAL_CODE = DP.HOSPITAL_CODE)
LEFT OUTER JOIN HOSPITAL_UNIT HU ON (DR.HOSPITAL_UNIT_CODE = HU.CODE AND
DR.HOSPITAL_CODE = HU.HOSPITAL_CODE)
LEFT OUTER JOIN VW_REVENUE_REPORT_DR SG ON (SM.DOCTOR_CODE = SG.GUARANTEE_DR_CODE AND
SM.HOSPITAL_CODE = SG.HOSPITAL_CODE AND SM.YYYY = SG.YYYY AND SM.MM = SG.MM)
INNER JOIN HOSPITAL HP ON SM.HOSPITAL_CODE  = HP.CODE 
WHERE SM.YYYY like $P{year}
AND SM.MM like $P{month}
AND SM.PAYMENT_TERM LIKE $P{term}
AND (SM.SUM_AMT + GUARANTEE_AMT + EXDR_AMT + EXCR_AMT > 0 OR SM.DR_NET_PAID_AMT > 0)
AND SM.PAYMENT_MODE_CODE LIKE $P{payment_mode_code}
AND DR.DOCTOR_TYPE_CODE LIKE $P{doctor_type_code}
AND SM.HOSPITAL_CODE LIKE $P{hospital_code}
AND DR.DEPARTMENT_CODE LIKE $P{doctor_department}
ORDER BY SM.PAYMENT_MODE_CODE,DP.CODE, SM.DOCTOR_CODE]]></queryString>

