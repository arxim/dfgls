	<queryString><![CDATA[SELECT     
	DESCRIPTION_THAI, COMPANY_NAME, OF_ADDRESSNO, OF_MOO, OF_BUILDER, OF_BUILDER_NUMBER, 
	OF_BUILDER_FLOOR, OF_VILLAGE, OF_SOI, OF_ROAD, OF_SUBDISTRICT,
	OF_DISTRICT, OF_PROVINCE, ZIP, TEL, FAX, REPLACE(TAXNO,'-','') AS TAXNO, ACCOUNT_NO, AUTHORITY_NAME, AUTHORITY_POSITION,
	(SELECT 
		SUM(ST.SUM_NORMAL_TAX_AMT)
	FROM SUMMARY_TAX_402 ST 
	WHERE YYYY = $P{yyyy} AND MM = '13' 
		AND ST.HOSPITAL_CODE = $P{hospital_code} AND ST.ACTIVE = '1') AS 'SUM_WITHOUT_TAX',
	(SELECT 
		SUM(ST.NET_TAX_MONTH)
	FROM SUMMARY_TAX_402 ST 
	WHERE YYYY = $P{yyyy} AND MM = '13' 
		AND ST.HOSPITAL_CODE = $P{hospital_code} AND ST.ACTIVE = '1') AS 'NET_TAX',
	(SELECT 
		COUNT(DOCTOR_CODE)
	FROM SUMMARY_TAX_402 ST 
	WHERE YYYY = $P{yyyy} AND MM = '13' 
		AND ST.SUM_NORMAL_TAX_AMT > 0
		AND ST.HOSPITAL_CODE = $P{hospital_code} AND ST.ACTIVE = '1') AS 'TOTAL_ROWS',
	(SELECT CASE WHEN (COUNT(DOCTOR_CODE)%7=0 OR COUNT(DOCTOR_CODE)%7=6) THEN
		CAST(CEILING(COUNT(DOCTOR_CODE)/7.0)+1 AS INT)
		ELSE CAST(CEILING(COUNT(DOCTOR_CODE)/7.0) AS INT) END
	FROM SUMMARY_TAX_402 ST 
	WHERE YYYY = $P{yyyy} AND MM = '13'
		AND SUM_NORMAL_TAX_AMT > 0
		AND ST.HOSPITAL_CODE = $P{hospital_code} AND ST.ACTIVE = '1') AS 'TOTAL_PAGES'
FROM         
	HOSPITAL AS HOS
WHERE CODE=$P{hospital_code}]]></queryString>

