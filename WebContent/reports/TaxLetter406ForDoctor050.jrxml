	<queryString><![CDATA[SELECT TAX.MM AS 'MM', TAX.DOCTOR_CODE AS 'DOCTOR_CODE', DOCTOR.NAME_THAI AS 'NAME_THAI',
TAX.SUM_TAX_DR_AMT AS REVENUE , 0.00 AS 'NET_TAX_MONTH',
TAX.TEXT_SUM_TAX_DR_AMT AS 'TEXT',
DOCTOR.ADDRESS1+' '+DOCTOR.ADDRESS2+' '+DOCTOR.ADDRESS3 AS 'ADDRESS2', DOCTOR.TAX_ID,
RIGHT('0000' + CONVERT(VARCHAR,ROW_NUMBER() OVER (ORDER BY TAX.YYYY, TAX.MM, DOCTOR.DOCTOR_PROFILE_CODE, TAX.DOCTOR_CODE)), 4) AS 'LINE_NO',
DOCTOR.DOCTOR_PROFILE_CODE,
HOSPITAL.COMPANY_NAME AS 'HOSPITAL_NAME', HOSPITAL.TAXNO, HOSPITAL.CODE AS HOSPITAL_CODE,
HOSPITAL.ADDRESS1+' '+HOSPITAL.ADDRESS2+' '+HOSPITAL.ADDRESS3 AS 'HOSPITAL_ADDRESS',
TAX.PRINT_DATE,
CONVERT(INT,TAX.YYYY)+1 AS PRINT_YEAR
FROM HOSPITAL LEFT OUTER JOIN SUMMARY_TAX_406 TAX ON HOSPITAL.CODE = TAX.HOSPITAL_CODE
LEFT OUTER JOIN DOCTOR ON TAX.DOCTOR_CODE = DOCTOR.CODE AND TAX.HOSPITAL_CODE = DOCTOR.HOSPITAL_CODE
WHERE TAX.SUM_TAX_DR_AMT > 0
AND TAX.DOCTOR_CODE LIKE $P{doctor}
AND DOCTOR.PAYMENT_MODE_CODE != 'U'
AND TAX.YYYY = $P{year}
AND TAX.HOSPITAL_CODE = $P{hospital_code}
AND TAX.PRINT_DATE <> ''
ORDER BY DOCTOR.DOCTOR_PROFILE_CODE, TAX.DOCTOR_CODE]]></queryString>

