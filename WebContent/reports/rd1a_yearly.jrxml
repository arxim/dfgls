	<queryString><![CDATA[SELECT ST.YYYY,ROW_NUMBER()OVER (ORDER BY ST.LINE_NO) AS SEQ, ST.DOCTOR_CODE, 
CASE WHEN ST.HOSPITAL_CODE = '054' THEN DR.BANK_ACCOUNT_NAME ELSE DR.NAME_THAI END AS NAME_THAI,
CASE WHEN LEN(DR.TAX_ID)>10 THEN DR.TAX_ID ELSE '' END AS NATION_ID, 
CASE WHEN LEN(DR.TAX_ID)<11 THEN DR.TAX_ID ELSE '' END AS TAX_ID,
ST.SUM_NORMAL_TAX_AMT, ST.NET_TAX_MONTH AS NET_TAX_MONTH, ST.YYYY, ST.MM, 
H.TAXNO,(DR.ADDRESS1+DR.ADDRESS2+DR.ADDRESS3) as ADDRESS, 
H.AUTHORITY_NAME, H.AUTHORITY_POSITION 
FROM SUMMARY_TAX_402 ST LEFT OUTER JOIN DOCTOR DR ON ST.DOCTOR_CODE = DR.CODE AND ST.HOSPITAL_CODE = DR.HOSPITAL_CODE
LEFT OUTER JOIN HOSPITAL H ON ST.HOSPITAL_CODE = H.CODE
WHERE ST.YYYY = $P{yyyy} AND ST.MM = '13' AND ST.HOSPITAL_CODE = $P{hospital_code}
AND ST.ACTIVE = '1'
AND DR.TAX_402_METHOD IN('STP', 'SUM')
ORDER BY SEQ, ST.NET_TAX_MONTH DESC]]></queryString>

