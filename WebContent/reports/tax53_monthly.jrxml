	<queryString><![CDATA[SELECT ROW_NUMBER()OVER (ORDER BY ST.NORMAL_TAX_MONTH DESC) AS SEQ, ST.DOCTOR_CODE, 
DR.NAME_THAI, CASE WHEN ST.TAX_402_METHOD = '' THEN DR.TAX_402_METHOD ELSE ST.TAX_402_METHOD END AS TAX_402_METHOD,
CASE WHEN LEN(DR.TAX_ID)>10 THEN DR.TAX_ID ELSE '' END AS NATION_ID,
DR.TAX_ID, DR.ADDRESS1+' '+DR.ADDRESS2+' '+DR.ADDRESS3+' '+DR.ZIP AS ADDRESS,
ST.SUM_NORMAL_TAX_AMT, ST.NET_TAX_MONTH AS NET_TAX_MONTH, ST.YYYY, ST.MM, H.TAXNO, H.COMPANY_NAME
FROM SUMMARY_TAX_402 ST LEFT OUTER JOIN DOCTOR DR ON ST.DOCTOR_CODE = DR.CODE AND ST.HOSPITAL_CODE = DR.HOSPITAL_CODE
LEFT OUTER JOIN HOSPITAL H ON ST.HOSPITAL_CODE = H.CODE
WHERE ST.YYYY = $P{yyyy} AND ST.MM = $P{mm} AND ST.HOSPITAL_CODE = $P{hospital_code}
AND ST.ACTIVE = '1'
AND SUM_NORMAL_TAX_AMT > 0
AND DR.TAX_402_METHOD NOT IN('STP','SUM')
AND DR.IS_LEGAL_ENTITY = 'Y'
ORDER BY SEQ, ST.NET_TAX_MONTH DESC]]></queryString>

